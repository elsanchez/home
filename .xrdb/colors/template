#define COLOR_FG                  #
#define COLOR_BG                  #

#define COLOR_TERM_BLACK          COLOR_FG
#define COLOR_TERM_DARK_RED       #
#define COLOR_TERM_DARK_GREEN     #
#define COLOR_TERM_DARK_YELLOW    #
#define COLOR_TERM_DARK_BLUE      #
#define COLOR_TERM_DARK_MAGENTA   #
#define COLOR_TERM_DARK_CYAN      #
#define COLOR_TERM_LIGHT_GRAY     COLOR_FG

#define COLOR_TERM_DARK_GRAY      COLOR_TERM_LIGHT_GRAY
#define COLOR_TERM_LIGHT_RED      COLOR_TERM_DARK_RED
#define COLOR_TERM_LIGHT_GREEN    COLOR_TERM_DARK_GREEN
#define COLOR_TERM_LIGHT_YELLOW   COLOR_TERM_DARK_YELLOW
#define COLOR_TERM_LIGHT_BLUE     COLOR_TERM_DARK_BLUE
#define COLOR_TERM_LIGHT_MAGENTA  COLOR_TERM_DARK_MAGENTA
#define COLOR_TERM_LIGHT_CYAN     COLOR_TERM_DARK_CYAN
#define COLOR_TERM_WHITE          #ffffff
